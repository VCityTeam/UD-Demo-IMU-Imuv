### Underlying mongo database

MONGO_INITDB_ROOT_USERNAME=default_username
MONGO_INITDB_ROOT_PASSWORD=default_password
MONGO_PORT=27017

### Exposed parse server

MONGO_HOST=mongodb
PARSE_APP_ID=default_application_identifier # also use in IMUV to connect to parse service
PARSE_MASTER_KEY=default_some_long_and_cryptic_and_stong_generated_key # also use in IMUV to connect to parse service
PARSE_PORT=1337

### Exposed imuv

IMUV_PORT=443

### Exposed wbo

WBO_PORT=5001

# Used by IMUV

JITSI_PUBLIC_URL=https://meet.jit.si
WBO_PUBLIC_URL=http://localhost:5001/boards
PARSE_SERVER_URL=http://parse-server:1337/parse
